        function fireImpBeacon(p_url, paramsObject){
                            	try{
                            		if(p_url && p_url != "null" && p_url != "undefined"){
                            			var timeStamp = new Date().getTime() + "";
                            			var clickTime = paramsObject ? paramsObject.ClickTime: 0;
